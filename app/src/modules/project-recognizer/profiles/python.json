{
  "name": "Python",
  "description": "General Python project footprint detected through dependency manifests.",
  "anyOfFiles": [
    "**/pyproject.toml",
    "**/requirements.txt"
  ],
  "manifests": [
    {
      "pathPattern": "**/pyproject.toml",
      "format": "text"
    },
    {
      "pathPattern": "**/requirements.txt",
      "format": "text"
    }
  ]
}
